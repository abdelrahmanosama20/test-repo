package Components;

import java.util.Hashtable;

public class RegFile {
    public String value;
    public String q; // Tag
    public String RegName;

    public RegFile(String value, String q, String regName) {
        this.value = value;
        this.q = q;
        RegName = regName;
    }

    @Override
    public String toString() {
        return "RegFile {value='" + value + 
               "', tag='" + q + 
               "', regName='" + RegName + "'}";
    }
}
